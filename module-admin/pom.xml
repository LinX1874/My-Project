    <modelVersion>4.0.0</modelVersion>
    <name>module-admin</name>

    <groupId>com.tangly</groupId>
    <artifactId>module-admin</artifactId>
    <version>1.0-SNAPSHOT</version>

    <dependencies>
        <dependency>
